{
  "functions": {
    "api/generate.js": {
      "runtime": "@vercel/node",
      "maxDuration": 15
    }
  },
  "rewrites": [
    { "source": "/api/generate", "destination": "/api/generate.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}