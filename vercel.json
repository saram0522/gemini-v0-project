{
  "builds": [
    {
      "src": "proxy-server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/proxy-server"
    }
  ],
  "outputDirectory": "public"
}